(event "Impossible" (when Wednesday Thursday))

(event "Sunday" Sunday)
(event "Monday" Monday)
(event "Tuesday" Tuesday)
(event "Wednesday" Wednesday)
(event "Thursday" Thursday)
(event "Friday" Friday)
(event "Saturday" Saturday)

(event "MT" (and Monday Tuesday))
(event "TW" (and Tuesday Wednesday))
(event "WT" (and Wednesday Thursday))

(event "midnight" midnight)

(event "MT (let)" (let t Tuesday (and Monday t) ))

(event "1am" (delay 1h midnight))
